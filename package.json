{
  "name": "zeiw-client",
  "license": "MIT",
  "scripts": {
    "build": "preact build --sw false --template src/index.html && npm run post-build",
    "post-build": "rm -rf build/{assets,polyfills*,manifest*} && cp -r src/cb.html build",
    "start": "preact watch --template src/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "dependencies": {
    "@tippy.js/react": "^3.1.1",
    "@zeiw/trump": "^1.0.7",
    "howler": "^2.1.2",
    "jss": "^10.0.0",
    "jss-camel-case": "^6.1.0",
    "jss-nested": "^6.0.1",
    "jss-vendor-prefixer": "^8.0.1",
    "micromodal": "0.4.0",
    "preact": "^10.0.5",
    "preact-cli": "^3.0.0-rc.6",
    "socket.io-client": "^2.3.0",
    "sweetalert2": "^9.3.14",
    "tachyons": "^4.11.1",
    "text-spinners": "^1.0.5",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  }
}
