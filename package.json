{
	"name": "zeiw-client",
	"license": "MIT",
	"scripts": {
		"post-build": "rm -rf build/{assets,polyfills*} && cp -r src/assets/* build",
		"build": "preact build --template src/index.html --no-prerender && npm run post-build",
		"start": "preact watch --template src/index.html"
	},
	"dependencies": {
		"@tippyjs/react": "4.0.4",
		"howler": "2.2.0",
		"js-cookie": "3.0.0-rc.0",
		"jss": "10.3.0",
		"jss-camel-case": "6.1.0",
		"jss-nested": "6.0.1",
		"jss-vendor-prefixer": "8.0.1",
		"micromodal": "0.4.6",
		"preact": "10.4.4",
		"preact-cli": "3.0.0-rc.7",
		"socket.io-client": "2.3.0",
		"sweetalert2": "9.15.1",
		"tachyons": "4.12.0"
	},
	"devDependencies": {
		"@types/howler": "2.2.0",
		"@types/js-cookie": "2.2.6",
		"@types/micromodal": "0.3.1",
		"@types/node": "14.0.13",
		"@types/prettier": "2.0.1",
		"@types/react": "16.9.38",
		"@types/socket.io-client": "1.4.33",
		"prettier": "2.0.5",
		"typescript": "3.9.5"
	},
	"prettier": {
		"semi": false,
		"useTabs": true,
		"trailingComma": "all"
	}
}
